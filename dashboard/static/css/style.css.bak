@import url('https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;1,300&display=swap');

body {
	font-family: 'Poppins', sans-serif !important;
    background-color: #fafafa;
    font-size: 100%;
}

a:link {
    outline: none;
}
.left { float: left; }
.right { float: right; }
.clear { clear: both; }
.container {
	width:99% !important;
	max-width:1500px !important;
	padding:0 !important;
	border:0px solid #f00;
}

header {
	height:50px;
	padding:5px 0;
	background-color:#fff;
	/*border-bottom:1px solid #e9e9e9;*/
}
.dm_header_logo {
	float: left;
	width:120px;
	border:0px solid #f00;
}
.dm_header_logo img {
	width:72px;
}
.dm_header_headtxt {
	float: left;
	border:0px solid #f00;
	display: inline-block;
	margin:0 auto;
	width:78%;
	text-align: center;
}
.dm_header_headtxt h1 {
	font-size:22px;
	padding:5px 0 0 0;
	margin:0;
	color:#555;
}
.dm_header_icons {
	float: right;
}
.dm_header_icons div {
	float:left;
	border:0px solid #999;
	width:30px; height:30px;
	line-height:30px !important;
	border-radius:4px;
	margin:4px 0 0 3px;
	text-align: center;
	opacity:0.8;
	
}
.dm_header_icons div:hover {
	opacity:1;
	cursor: pointer;
}
.dm_header_icons div:nth-child(1) img{
	width:68%;	
	margin:4px 0 0 0;
}
.dm_header_icons div:nth-child(2) img{
	width:60%;	
	margin:5px 0 0 0;
}
.dm_header_icons div:nth-child(3) img{
	width:66%;	
	margin:5px 0 0 0;
}
.dm_header_icons div:nth-child(4) img{
	width:60%;	
	margin:5px 0 0 0;
}

.dm_breadcrumb {
	border:0px solid #f00;
	padding:12px 0 8px 0;
}

.dm_breadcrumb h3 {
	padding:0; margin:0;
	font-size:20px;
	font-weight: normal;
	color:#666666;
}

.dm_summary {
	border:0px solid #f00;
	margin:5px 0.6% 12px;	
}
.dm_summary .dm_summary_box {
	border:0px solid #f00;
	width:20%;
	float:left;
	text-align: center;
}
.dm_summary .dm_summary_box p {
	padding:5px 0; margin:0;
	font-size:16px;
	color:#888;
	font-weight: 300;
}
.dm_summary .dm_summary_box h2 {
	padding:2px 0; margin:0;
	font-size:30px;
	color:#222;
	font-weight: normal;
}

.dm_chart_contain {
	background-color:#fff;
}
.dm_chartbox {
	background-color:#fff;
	padding:10px;
	border:1px solid #f00;
	float:left;
	min-height:378px;
}
.dm_chartbox  h4 {
	font-size:17px;
	font-weight:400;
	color:#444;
	padding:0 0 20px 0; margin:0;
}
.dm_chartbox .dm_chart_area {
	border:0px solid #f00;
	text-align: center;
}

.dm_twoclmn {
	margin:10px 0;
	width:50%;
}

.dm_threeclmn {
	margin:10px 0;
	width:33.333%;
}

.dm_fourclmn {
	margin:10px 0;
	width:25%;
}


.dm39prcnt_box {
	margin:10px 0;
	width:39%;
}
.dm22prcnt_box {
	margin:10px 0;
	width:22%;
}
.dm40prcnt_box {
	margin:10px 0;
	width:40%;
}
.dm30prcnt_box {
	margin:10px 0;
	width:30%;
}










